{
  "routes": [
    {
      "src": "/Songs/(.*)",
      "dest": "/Songs/$1"
    }
  ],
  "headers": [
    {
      "source": "/Songs/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Content-Type",
          "value": "audio/mpeg"
        }
      ]
    }
  ]
}
